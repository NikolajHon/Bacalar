"latex-workshop.latex.toolchain": [
    {
        "command": "pdflatex",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "%DOC%"
        ]
    }
],
"latex-workshop.latex.outDir": "./out"  // это путь, куда будут сохраняться PDF-файлы
